/*----------------------------------------*\
  TRANSCRIPT
\*----------------------------------------*/

.transcript {
  @apply max-w-2xl mx-auto;
}

.transcript__row {
  @apply py-4 border-t border-elements-divider-50;
}

.transcript__speaker {
  @apply font-bold;
}

.transcript__timestamp {
  @apply text--sm text-text-new-label;
}
